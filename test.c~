#include <stdio.h>
#include "status.h"
#include "stack.h"

int main(int argc, char *argv[])
{
    stack_init(Stack);
    
    stack_push(Stack, 1);
    stack_push(Stack, 2);
    stack_push(Stack, 3);
    stack_push(Stack, 4);
    printf("test:%d\n", Stack[1]);

    stack_elem ret = 0;
    stack_pop(Stack, &ret);
    printf("%d\n", ret);
    stack_pop(Stack, &ret);
    printf("%d\n", ret);
    stack_pop(Stack, &ret);
    printf("%d\n", ret);
    stack_pop(Stack, &ret);
    printf("%d\n", ret);

    return 0;
}
